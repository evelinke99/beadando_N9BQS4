from math import sqrt

def prim(szam):
    for i in range(2,int(sqrt(szam))+1):
        if szam % i == 0:
            return False
    return True

def nik_prim(a):                                           #KÉÉÉSZ*-*
    n = 0
    prim = 0
    i = 2
    while n<a:
        if prim(i):
            n+=1
            prim = i
        i+=1
    return prim
    
print(nik_prim(10001)
